{
  "name": "paystation",
  "version": "1.0.0",
  "description": "Paystation - Point of sale system",
  "keywords": [
    "Point of sale",
    "Payment",
    "Cryptocurrency"
  ],
  "license": "MIT",
  "author": "Paul Puey <paul@vtex.net>",
  "main": "lib/index.js",
  "scripts": {
    "build.web": "webpack --mode production",
    "build.server": "tsc -p tsconfig.server.json",
    "build": "yarn build.server",
    "clean": "rimraf lib dist",
    "configure": "configure src/server/serverConfig.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "fix": "eslint . --ext .ts,.tsx --fix",
    "precommit": "yarn lint && yarn build",
    "prepare": "husky install && npm-run-all clean -p build.*",
    "start": "node -r sucrase/register src/server/index.ts",
    "start.engine": "node -r sucrase/register src/server/indexEngine.ts",
    "start.web": "webpack serve --mode development",
    "setup": "node -r sucrase/register src/server/dbInit.ts",
    "test": "mocha -r sucrase/register 'src/__tests__/**/*.ts'",
    "types": "yarn build"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint"
  },
  "dependencies": {
    "cleaners": "^0.3.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native-patina": "^0.1.6",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.26.0",
    "@types/chai": "^4.2.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/mocha": "^5.2.7",
    "@types/nano": "^7.0.0",
    "@types/node": "^24.3.1",
    "@types/node-fetch": "^2.5.3",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/universal-analytics": "^0.4.3",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "babel-loader": "^9.2.1",
    "babel-plugin-react-native-web": "^0.19.12",
    "chai": "^3.5.0",
    "cleaner-config": "^0.1.10",
    "copy-webpack-plugin": "^13.0.1",
    "cors": "^2.8.5",
    "css-loader": "^7.1.2",
    "edge-server-tools": "0.2.22",
    "eslint": "^9.32.0",
    "eslint-config-standard-kit": "^1.0.0",
    "express": "^5.1.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.1.7",
    "lint-staged": "^10.5.3",
    "mocha": "^6.2.2",
    "nano": "^10.1.0",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "puppeteer": "^24.25.0",
    "qrcode-generator": "^1.4.4",
    "react-native": "^0.76.6",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-web": "^0.19.13",
    "react-native-web-linear-gradient": "^1.1.2",
    "react-router-dom": "^7.9.4",
    "rimraf": "^3.0.0",
    "serverlet": "^0.1.3",
    "style-loader": "^4.0.0",
    "sucrase": "^3.35.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.0",
    "universal-analytics": "^0.4.17",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
